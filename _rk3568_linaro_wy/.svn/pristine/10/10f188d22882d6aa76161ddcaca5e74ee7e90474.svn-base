<?xml version="1.0" encoding="utf-8"?>
<config>
    <log_enable>0</log_enable>              <!-- whether enable the log function, 0-disable, 1-enable -->
    <log_level>5</log_level>                <!-- 0:TRACE,1:DEBUG,2:INFO,3:WARNING,4:ERROR,5:FATAL --> 
    <loop_nums>-1</loop_nums>               <!-- when pushing local media files, specify the number of loop, -1 means infinite loop -->
    <reconn_interval>3</reconn_interval>   <!-- When the connection with the server is disconnected, the interval between reconnecting to the server, unit is second -->
    
    <pusher>                                <!-- Represents a push stream, specify the audio and video output parameters, it can configure multiple nodes -->
        <srcurl>live</srcurl>           <!-- The push stream source, local files or rtsp/rtmp/http mjpeg url or specify device url -->          
        <dsturl>rtmp://183.230.64.58/live/3.0</dsturl> <!-- Specify the push destination address, it should be a RTMP stream address -->
        <video>                             <!-- Specify the video output parameters -->
            <codec>H264</codec>             <!-- Specify the output video codec, H264,H265 -->
            <width></width>                 <!-- Specify the output video width, if 0 it use the original video width (living screen use the screen width, camera use the default width) -->
            <height></height>               <!-- Specify the output video height, if 0 it use the original video height (living screen use the screen height, camera use the default height) -->
            <framerate></framerate>         <!-- Specify the output video framerate, if 0 it use the original video framerate (living screen use the default value 15, camera use the default value 25) -->
            <bitrate></bitrate>             <!-- Specify the output video bit rate, if it is 0, it is automatically calculated -->
        </video>
        <audio>                             <!-- Specify the audio output parameters -->
            <codec>AAC</codec>              <!-- Specify the output audio codec, AAC,G711A,G711U -->
            <samplerate>44100</samplerate>  <!-- Specify the audio sample rate, If 0 it use the original audio sample rate (audio device use the default value 8000) -->
            <channels>2</channels>          <!-- Specify the audio channel number, 1 is mono, 2 is stereo -->
            <bitrate></bitrate>             <!-- Specify the output audio bit rate, if it is 0, it is automatically calculated -->
        </audio>
    </pusher>
    
</config>


    
